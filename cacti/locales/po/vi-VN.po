msgstr "Chọn hộp này để vô hiệu hóa tất cả các kiểm tra cho máy chủ này."

#: include/global_form.php:1258
msgid "Availability/Reachability Options"
msgstr "Tùy chọn sẵn có / Khả năng tiếp cận"

#: include/global_form.php:1262 include/global_settings.php:774
msgid "Downed Device Detection"
