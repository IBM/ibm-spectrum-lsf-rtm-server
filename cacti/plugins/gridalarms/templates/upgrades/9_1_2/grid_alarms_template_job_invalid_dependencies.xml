			<hash_21002196e658be6050f393dc2a4cf405602a67>
				<id>16</id>
				<name>Invalid Dependencies</name>
				<description>Shows jobs with invalid dependencies</description>
				<ds_type>1</ds_type>
				<type>1</type>
				<db_table>grid_jobs</db_table>
				<sql_query>SELECT gc.clustername, gj.*
FROM grid_clusters AS gc
INNER JOIN grid_jobs AS gj
ON gc.clusterid=gj.clusterid
WHERE gj.stat=&#039;PEND&#039;
AND gj.pendReasons like &#039;%Dependency%invalid%&#039;</sql_query>
