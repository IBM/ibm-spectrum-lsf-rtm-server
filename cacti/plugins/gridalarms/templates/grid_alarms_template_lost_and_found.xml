			<hash_210021fb1383f2ed658c9c105867f1911b1595>
				<id>19</id>
				<name>lost_and_found</name>
				<description>A job is moved to the lost_and_found queue or lost_and_found host.</description>
				<ds_type>1</ds_type>
				<type>1</type>
				<db_table></db_table>
				<sql_query>SELECT gc.clustername, gj.*
FROM grid_jobs AS gj
INNER JOIN grid_clusters AS gc
ON gc.clusterid=gj.clusterid
WHERE queue=&#039;lost_and_found&#039;
OR exec_host=&#039;lost_and_found&#039;</sql_query>
