<!-- $Id$ -->
<interface>
	<name>Disk Utilization Organization Information</name>
	<script_path>|path_cacti|/scripts/ss_disku_orgs.php</script_path>
	<script_function>ss_disku_orgs</script_function>
	<script_server>php</script_server>
	<arg_prepend></arg_prepend>
	<arg_index>index</arg_index>
	<arg_query>query</arg_query>
	<arg_get>get</arg_get>
	<output_delimeter>!</output_delimeter>
	<index_order>orgId</index_order>
	<index_order_type>alpha</index_order_type>
	<index_title_format>|chosen_order_field|</index_title_format>
	<fields>
	        <orgId>
	                <name>Organization ID</name>
	                <direction>input</direction>
	                <query_name>orgId</query_name>
	        </orgId>
	        <orgName>
	                <name>Organization Name</name>
	                <direction>input</direction>
	                <query_name>orgName</query_name>
	        </orgName>
	        <totalFiles>
	                <name>Total Files</name>
	                <direction>output</direction>
	                <query_name>files</query_name>
	        </totalFiles>
	        <totalDirs>
	                <name>Total Directories</name>
	                <direction>output</direction>
	                <query_name>dirs</query_name>
	        </totalDirs>
	        <totalSize>
	                <name>Size on Disk</name>
	                <direction>output</direction>
	                <query_name>size</query_name>
	        </totalSize>
	        <totalUsers>
	                <name>Unique Active Users</name>
	                <direction>output</direction>
	                <query_name>users</query_name>
	        </totalUsers>
	</fields>
</interface>
