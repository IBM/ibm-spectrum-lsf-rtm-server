<!-- $Id$ -->
<interface>
	<name>Disk Utilization User Group Information</name>
	<script_path>|path_cacti|/scripts/ss_disku_groups.php</script_path>
	<script_function>ss_disku_groups</script_function>
	<script_server>php</script_server>
	<arg_prepend></arg_prepend>
	<arg_index>index</arg_index>
	<arg_query>query</arg_query>
	<arg_get>get</arg_get>
	<output_delimeter>!</output_delimeter>
	<index_order>groupId</index_order>
	<index_order_type>numeric</index_order_type>
	<index_title_format>|chosen_order_field|</index_title_format>
	<fields>
	        <groupId>
	                <name>Group ID</name>
	                <direction>input</direction>
	                <query_name>groupid</query_name>
	        </groupId>
	        <groupName>
	                <name>Group Name</name>
	                <direction>input</direction>
	                <query_name>groupName</query_name>
	        </groupName>
	        <totalFiles>
	                <name>Total Files</name>
	                <direction>output</direction>
	                <query_name>files</query_name>
	        </totalFiles>
	        <totalDirs>
	                <name>Total Directories</name>
	                <direction>output</direction>
	                <query_name>dirs</query_name>
	        </totalDirs>
	        <totalSize>
	                <name>Size on Disk</name>
	                <direction>output</direction>
	                <query_name>size</query_name>
	        </totalSize>
	        <totalUsers>
	                <name>Unique Active Users</name>
	                <direction>output</direction>
	                <query_name>users</query_name>
	        </totalUsers>
	</fields>
</interface>
